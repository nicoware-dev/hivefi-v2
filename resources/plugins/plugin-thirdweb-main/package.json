{
    "name": "@elizaos-plugins/plugin-thirdweb",
    "version": "0.1.9",
    "main": "dist/index.js",
    "type": "module",
    "types": "dist/index.d.ts",
    "dependencies": {
        "thirdweb": "^5.80.0",
        "tsup": "8.3.5"
    },
    "devDependencies": {
        "@biomejs/biome": "1.5.3",
        "tsup": "^8.3.5"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "lint": "biome check src/",
        "lint:fix": "biome check --apply src/",
        "format": "biome format src/",
        "format:fix": "biome format --write src/"
    },
    "peerDependencies": {
        "whatwg-url": "7.1.0"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "THIRDWEB_SECRET_KEY": {
                "type": "string",
                "description": "ThirdWeb secret key for API authentication"
            }
        }
    }
}